<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.FirstPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="Entry1" Placeholder="Write"></Entry>
            <Button Text="Click" Clicked="Button_Clicked"></Button>
            <Label x:Name ="Label1" Text="Привет"></Label>
            <Button x:Name="Button_level" Text="Play" Clicked ="Button_level_Clicked"></Button>
            <Button x:Name="Button_Setting" Text="Settings" Clicked = "Button_Setting_Clicked"></Button>
            <Button x:Name="Button_exit" Text="Exit" Clicked ="Button_exit_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>